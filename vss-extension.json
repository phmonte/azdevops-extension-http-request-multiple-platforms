{
    "manifestVersion": 1,
    "id": "SimpleHttpRequest",
    "version": "1.1.5",
    "name": "Smoke Test Multiplatform",
    "publisher": "phmonte",
    "description": "This task makes a smoke test request and wait status code 200",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "content": {
        "details": {
            "path": "MarketPlace.md"
        },
        "license": {
            "path": "license.txt"
        },
        "privacypolicy": {
            "path": "privacy.md"
        }
    },
    "icons": {
        "default": "img/icon.png"
    },
    "categories": [
        "Azure Pipelines"
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/phmonte/azdevops-extension-http-request-multiple-platforms"
    },
    "contributions": [
        {
            "id": "SimpleHttpRequest",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "SimpleHttpRequest"
            }
        }
    ],
    "files": [
        {
            "path": "SimpleHttpRequest"
        },
        {
            "path": "img",
            "addressable": true
        }
    ],
    "galleryFlags": [
        "Public"
    ]
}